    "less": "^4.1.2",
    "lint-staged": "^12.4.1",
    "postcss-less": "^6.0.0",
    "prettier": "^2.6.2",
    "stylelint": "^14.12.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^28.0.0",
    "typescript": "^4.6.3",
    "vite": "^4.1.0",
